#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp A  &kp B  &mo 1
&kp C  &kp D  &kp E
            >;
        };

        bt_layer {
            bindings = <
 &bootloader         &none       &trans
&bt BT_SEL 0  &bt BT_SEL 1  &bt BT_CLR
            >;
        };
    };
};
